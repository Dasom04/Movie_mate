<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ictproject.moviemate.domain.user.mapper.UserMapper">
    <insert id="join" >
       insert into user (profile,email,nick_name,login_path) values
        (#{profile},#{email},#{nick_name},#{login_path})
    </insert>

    <select id="isExist" resultType="boolean">
        select count(*) from user where email = #{email} and login_path = #{loginPath}
    </select>
</mapper>